            android:id="@+id/action_ViewFragment_to_HomeFragment"
            app:destination="@id/HomeFragment" />
        <action
            android:id="@+id/action_ViewFragment_to_RankFragment"
            app:destination="@id/RankFragment" />

    </fragment>
    <fragment
        android:id="@+id/MapFragment"
        android:name="com.example.tourapp.fragment.MapFragment"
        android:label="@string/map_fragment_label"
